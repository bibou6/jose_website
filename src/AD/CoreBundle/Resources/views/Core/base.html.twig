<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta name="viewport"
	content="width=device-width, initial-scale=1.0, minimal-ui">
<meta name="Description"
	content="Joe website for paintings and drawings">
<meta name="theme-color" content="#d66e00" />
<title>{% block title %}Welcome!{% endblock %}</title>
{% block stylesheets %}
{# 'app' must match the first argument to addEntry() in
webpack.config.js #}
<link rel="stylesheet"
	href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
	integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
	crossorigin="anonymous">
{% stylesheets filter='cssrewrite,?scssphp' 
		'bundles/core/css/*' %}
<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
{% endstylesheets %}
{% endblock %}

<style>
{% block custom_css %}

{% endblock %}
</style>

</head>
<body>
	{% block menu %}
	{% endblock %}
	{% block flashbag %}
	{% for label, messages in app.flashes() %}
	{% for message in messages %}
	<div class="flashbag alert alert-{{ label }}">
		{{ message }}
	</div>
	{% endfor %}
	{% endfor %}
	{% endblock %}


	{% block body %}
	{% endblock %}
	{% block javascripts %}
	{% javascripts filter='jsqueeze' 'bundles/core/js/*' %}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
	<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
		integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
		crossorigin="anonymous"></script>
	<script
		src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
		integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
		crossorigin="anonymous"></script>
	<script
		src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
		integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
		crossorigin="anonymous"></script>
	{% endblock %}

</body>
</html>
